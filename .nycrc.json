{
    "all": true,
    "branches": 0,
    "lines": 0,
    "functions": 0,
    "statements": 0,
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ],
    "report-dir": "./reports/code.coverage/",
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "cache": false,
    "source-map": true,
    "produce-source-map": true,
    "exclude-after-remap": false
}