{
  "name": "es6.dictionary",
  "version": "1.0.4",
  "description": "ES6  Dictionary Implemented in Typescript",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types":"./lib/esm/index.d.ts",
  "scripts": {
    "clean.lib.esm": "node ./tasks/clean.lib.esm.js",
    "clean.lib.cjs": "node ./tasks/clean.lib.cjs.js",
    "clean.test": "node ./tasks/clean.test.js",
    "build.lib.esm": "npm run clean.lib.esm && tsc -p ./tasks/build.lib.esm.json",
    "build.lib.cjs": "npm run clean.lib.cjs && tsc -p ./tasks/build.lib.cjs.json",
    "build.lib": "npm run build.lib.esm && npm rum build.lib.cjs",
    "build.test": "npm run clean.test && tsc -p ./tasks/build.test.json",
    "pretest": "npm run build.test",
    "test": "nyc mocha --config .mocharc.json",
    "posttest": "npm run clean.test",
    "prepublish": "npm run build.lib"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "Front-end",
    "ES6",
    "Typescript",
    "Data Structure",
    "Dictionary"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlippieCoetser/Dictionary.git"
  },
  "author": "PJ Coetser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlippieCoetser/Dictionary/issues"
  },
  "homepage": "https://github.com/FlippieCoetser/Dictionary#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@types/sinon": "^9.0.5",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "node-cmd": "^3.0.0",
    "node-delete": "^0.1.2",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.2"
  }
}
